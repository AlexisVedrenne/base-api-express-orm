{
  "name": "api-base",
  "version": "1.0.0",
  "private": true,
  "main": "src/bin/index.js",
  "scripts": {
    "dev": "nodemon ./src/bin/index.js",
    "start": "node /src/bin/index.js",
    "build": "pkg .",
    "model:create":"sequelize-cli model:generate --name",
    "migration:gen":"sequelize-cli migration:generate --name",
    "migrate":"sequelize-cli db:migrate",
    "seed:create":"sequelize-cli seed:generate --name",
    "db:create":"sequelize-cli db:create",
    "seed:gen:all":"sequelize-cli db:seed:all"
  },
  "bin": "src/bin/index.js",
  "pkg": {
    "assets": [
      "src/config/.env",
      "src/boot/**/*.js",
      "src/config/**/*.js",
      "src/controllers/**/*.js",
      "src/middleware/**/*.js",
      "src/models/**/*.js",
      "src/routes/**/*.js",
      "src/services/**/*.js"
    ],
    "scripts": [
      "src/bin/**/*.js"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.3",
    "node-mailjet": "^6.0.9",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.0.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "systeminformation": "^5.21.17",
    "tedious": "^16.6.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "sequelize-cli": "^6.6.3"
  }
}
